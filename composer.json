{
    "name": "lbausch/ceph-radosgw-admin",
    "description": "PHP Ceph Radosgw Admin",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lorenz Bausch",
            "email": "info@lorenzbausch.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "aws/aws-sdk-php": "^3.131",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4",
        "rector/rector": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "LBausch\\CephRadosgwAdmin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
